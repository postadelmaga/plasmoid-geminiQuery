msgstr "Aggiorna"

#: contents/ui/main.qml:134
msgid "Refresh response"
msgstr "Aggiorna risposta"

#: contents/ui/main.qml:139
msgid "Configure"
msgstr "Configura"

#: contents/ui/main.qml:143
msgid "Settings"
msgstr "Impostazioni"

#: contents/ui/main.qml:202
msgid "No response in cache.\nPress 'Refresh' to query Gemini."
msgstr "Nessuna risposta in cache.\nPremi 'Aggiorna' per interrogare Gemini."

#: contents/ui/main.qml:223
msgid "Cache from: %1"
msgstr "Cache del: %1"

#: contents/ui/configGeneral.qml:23
msgid "Widget Title:"
msgstr "Titolo del Widget:"

#: contents/ui/configGeneral.qml:25
msgid "Enter a custom title (e.g. Daily Quote)"
msgstr "Inserisci un titolo personalizzato (es: Citazione del Giorno)"

#: contents/ui/configGeneral.qml:35
msgid "Gemini API Key:"
msgstr "API Key di Gemini:"

#: contents/ui/configGeneral.qml:41
msgid "Enter your Google Gemini API Key"
msgstr "Inserisci la tua API Key di Google Gemini"

#: contents/ui/configGeneral.qml:47
msgid "Show"
msgstr "Mostra"

#: contents/ui/configGeneral.qml:53
msgid "Gemini Model:"
msgstr "Modello Gemini:"

#: contents/ui/configGeneral.qml:55
msgid "e.g. gemini-1.5-flash-latest"
msgstr "es: gemini-1.5-flash-latest"

#: contents/ui/configGeneral.qml:71
msgid "Get a free API Key at: <a href='%1'>Google AI Studio</a>"
msgstr "Ottieni una API Key gratuita su: <a href='%1'>Google AI Studio</a>"

#: contents/ui/configGeneral.qml:91
msgid "Question to ask:"
msgstr "Domanda da porre:"

#: contents/ui/configGeneral.qml:99
msgid "Ex: What is the motivational quote of the day?"
msgstr "Es: Qual è la frase motivazionale del giorno?"

#: contents/ui/configGeneral.qml:105
msgid "This question will be sent to Gemini once a day"
msgstr "Questa domanda verrà inviata a Gemini una volta al giorno"

#: contents/ui/configGeneral.qml:118
msgid "Example questions:"
msgstr "Esempi di domande:"

#: contents/ui/configGeneral.qml:126
msgid "Give me a motivational quote to start the day well"
msgstr "Dammi una frase motivazionale per iniziare bene la giornata"

#: contents/ui/configGeneral.qml:127
msgid "What is an interesting fact from today in history?"
msgstr "Qual è un fatto interessante di oggi nella storia?"

#: contents/ui/configGeneral.qml:128
msgid "Give me a productivity tip for today"
msgstr "Dammi un consiglio di produttività per oggi"

#: contents/ui/configGeneral.qml:129
msgid "Suggest a quick recipe for dinner"
msgstr "Suggeriscimi una ricetta veloce per la cena"

#: contents/ui/configGeneral.qml:130
msgid "What is the word of the day with its definition?"
msgstr "Qual è la parola del giorno con la sua definizione?"

